
# >>> conda initialize >>>
# !! Contents within this block are managed by 'conda init' !!
__conda_setup="$('/Users/leon/opt/anaconda3/bin/conda' 'shell.bash' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "/Users/leon/opt/anaconda3/etc/profile.d/conda.sh" ]; then
        . "/Users/leon/opt/anaconda3/etc/profile.d/conda.sh"
    else
        export PATH="/Users/leon/opt/anaconda3/bin:$PATH"
    fi
fi

if [ -f ~/.bashrc ]; then
    source ~/.bashrc
fi

if type brew &> /dev/null; then
    HOMEBREW_PREFIX=$(brew --prefix)
    for d in ${HOMEBREW_PREFIX}/opt/*/libexec/gnubin; do
		export PATH=$d:$PATH;
    done;
    for d in ${HOMEBREW_PREFIX}/opt/*/libexec/gnuman; do
		export MANPATH=$d:$MANPATH;
    done;
fi;

unset __conda_setup
# <<< conda initialize <<<

